local Players = game:GetService('Players')
local LocalPlayer = Players.LocalPlayer
local VirtualUser = game:GetService('VirtualUser')
local PlaceId = game.PlaceId

-- Key system
local userKey = "Jahceere" -- Change dynamically via GUI later
local mode = "FREE"

if userKey == "JahP" then
    mode = "PREMIUM"
    print("üîë Script Kiddies: Premium Key Accepted. Loading premium scripts.")
elseif userKey == "Jahceere" then
    mode = "UNPAID"
    print("üîì Script Kiddies: Unpaid user mode activated. Loading unpaid user scripts.")
else
    warn("‚ö†Ô∏è Invalid key provided. No scripts will load.")
end

-- Universal Anti-AFK
spawn(function()
    while true do
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
        task.wait(30)
    end
end)

-- Loader function for clarity
local function loadScript(url, gameName)
    local success, result = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    if success then
        print("‚úÖ Script Kiddies: " .. gameName .. " script loaded.")
    else
        warn("‚ùå Script Kiddies: Failed to load " .. gameName .. " script:", result)
    end
end

